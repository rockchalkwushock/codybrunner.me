{
  "name": "codybrunner.me",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Cody Brunner <rockchalkwushock@icloud.com> (https://codybrunner.me)",
  "description": "My personal website & portfolio built with Gatsby.",
  "repository": "https://github.com/rockchalkwushock/codybrunner.me.git",
  "scripts": {
    "build": "gatsby build",
    "cm": "git cz",
    "develop": "gatsby develop",
    "serve": "gatsby serve"
  },
  "dependencies": {

  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.16.0",
    "eslint-config-equimper": "^2.2.2",
    "eslint-config-prettier": "^2.9.0",
    "husky": "^0.15.0-rc.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "equimper",
      "prettier"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
  }
}
